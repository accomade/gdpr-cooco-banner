		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@types/js-cookie": "^3.0.6",
		"publint": "^0.1.16",
		"svelte": "^5.19.0",
		"svelte-check": "^4.1.4",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vite": "^5.4.11"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"js-cookie": "^3.0.5"
	},
	"peerDependencies": {
		"svelte": "^5"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
